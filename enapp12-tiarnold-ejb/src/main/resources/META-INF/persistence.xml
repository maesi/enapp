<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="enapp12-tiarnold-pu" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/enappwebshop</jta-data-source>
		<class>org.maesi.hslu.enapp.entity.Customer</class>
		<class>org.maesi.hslu.enapp.entity.Product</class>
		<class>org.maesi.hslu.enapp.entity.Purchase</class>
		<class>org.maesi.hslu.enapp.entity.Purchaseitem</class>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.jdbc2.optional.MysqlDataSource"/>
		</properties>
		<!-- <class>org.maesi.hslu.enapp.entity.Product</class>
		<class>org.maesi.hslu.enapp.entity.Purchase</class>
		<class>org.maesi.hslu.enapp.entity.Purchaseitem</class>
		<exclude-unlisted-classes>false</exclude-unlisted-classes> -->
	</persistence-unit>
</persistence>
